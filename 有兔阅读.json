{
  "name": "有兔阅读",
  "version": 100,
  "category": 1,
  "url": "https://app.youzibank.com",
  "charset": "utf-8",
  "metadata": {
    "name": ["$.name"],
    "author": ["$.author", "$.data[0].author"],
    "cover": [
      "$.photoPath@js->'https://book.chengxinqinye.com/book' + ${this}",
      "$.data[0].photoPath@js->'https://book.chengxinqinye.com/book' + ${this}"
    ],
    "summary": ["$.intro", "$.data[0].intro"],
    "category": ["", "$.data[0].clsName"],
    "update": ["$.data[0].createTime"],
    "link": [
      "$.id@js->'http://app.youzibank.com/book/info?bookId='+${this}@@header->Seq=3b28f4be398947da273c3d1ec2c2fb8b",
      "$.data[0].id@js->'http://app.youzibank.com/book/info?bookId='+${this}@@header->Seq=3b28f4be398947da273c3d1ec2c2fb8b"
    ],
    "catalog": [
      "$.id@js->http://app.youzibank.com/book/chapter/listAll?bookId=' + ${this}",
      "$.data[0].id@js->http://app.youzibank.com/book/chapter/listAll?bookId=' + ${this}"
    ]
  },
  "catalog": {
    "list": "$.data",
    "orderBy": 0,
    "chapter": {
      "name": "$.name",
      "link": "$.filePath@js->'http://app.youzibank.com/book' + ${this}"
    }
  },
  "content": {
    "text": ""
  },
  "search": {
    "link": "${key}@js->'http://app.youzibank.com/es/search/book?pageNo=1&pageSize=10&q=' + encodeURIComponent(${this})",
    "list": "$.data"
  },
  "rank": {
    "link": [
      {
        "name": "人气榜",
        "link": "http://app.youzibank.com/popular/new/detail?pageSize=10&type=1&pageNo="
      },
      {
        "name": "女频榜",
        "link": "http://app.youzibank.com/popular/new/detail?pageSize=10&type=2&pageNo="
      },
      {
        "name": "男频榜",
        "link": "http://app.youzibank.com/popular/new/detail?pageSize=10&type=3&pageNo="
      },
      {
        "name": "完本榜",
        "link": "http://app.youzibank.com/popular/new/detail?pageSize=10&type=4&pageNo="
      },
      {
        "name": "完本榜",
        "link": "http://app.youzibank.com/popular/new/detail?pageSize=10subType=1&&type=5&pageNo="
      },
      {
        "name": "评分榜",
        "link": "http://app.youzibank.com/popular/new/detail?pageSize=10subType=1&&type=6&pageNo="
      }
    ],
    "list": "$.data",
    "page": {
      "index": 1,
      "limit": 10,
      "begin": "",
      "next": "${index}"
    }
